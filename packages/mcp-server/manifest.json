{
  "dxt_version": "0.2",
  "name": "benchify-mcp",
  "version": "0.1.0-alpha.62",
  "description": "The official MCP Server for the Benchify API",
  "author": {
    "name": "Benchify",
    "email": "support@benchify.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Benchify/benchify-sdk.git"
  },
  "homepage": "https://github.com/Benchify/benchify-sdk/tree/main/packages/mcp-server#readme",
  "documentation": "https://benchify.com/support",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "BENCHIFY_API_KEY": "${user_config.BENCHIFY_API_KEY}"
      }
    }
  },
  "user_config": {
    "BENCHIFY_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": false,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
